{
  "name": "@limbar/api",
  "version": "0.1.0",
  "private": false,
  "description": "TypeScript API clients for Limbar endpoints that let you control Android Emulators and iOS Simulators from the browser.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./region": {
      "types": "./dist/region/index.d.ts",
      "import": "./dist/region/index.js",
      "require": "./dist/region/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "generate": "oazapfts openapi/region.yaml src/region/zz_client.ts"
  },
  "dependencies": {
    "oazapfts": "^6.2.2",
    "openapi-fetch": "^0.13.5"
  },
  "keywords": [
    "api",
    "limbar"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/limbario/ui",
    "directory": "packages/api"
  },
  "author": "Limbar",
  "license": "MIT"
}
